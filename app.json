{
  "expo": {
    "name": "FoodBuddy",
    "slug": "FoodBuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/startimg.jpg",
    "scheme": "foodbuddy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "image": "./assets/images/startimg.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/startimg.jpg",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/startimg.jpg",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/startimg.jpg"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}